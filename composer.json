{
    "name": "daniel-km/omeka-s-module-data-type-rdf",
    "type": "omeka-s-module",
    "description": "Module for Omeka S: Implement some  W3C RDF datatypes (json, html, xml, boolean) in order to simplify user input and to give more semanticity to values of resources.",
    "keywords": [
        "Omeka S",
        "Omeka S module",
        "semantic web",
        "rdf",
        "xsd",
        "w3c recommendation",
        "linked data"
    ],
    "license": "CECILL-2.1",
    "homepage": "https://gitlab.com/Daniel-KM/Omeka-S-module-DataTypeRdf",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.packagist@Berthereau.net"
        }
    ],
    "support": {
        "issues": "https://gitlab.com/Daniel-KM/Omeka-S-module-DataTypeRdf/-/issues",
        "forum": "https://forum.omeka.org/"
    },
    "_require (not set until Omeka S merges PR #2412)": {
        "omeka/omeka-s": "^4.0",
        "daniel-km/omeka-s-module-common": "*"
    },
    "require": {
        "ext-xml": "*",
        "sempia/external-assets": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DataTypeRdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DataTypeRdfTest\\": "tests/DataTypeRdfTest/"
        }
    },
    "config": {
        "allow-plugins": {
            "sempia/external-assets": true
        }
    },
    "extra": {
        "installer-name": "DataTypeRdf",
        "external-assets": {
            "asset/vendor/ckeditor-footnotes/": "https://github.com/Daniel-KM/Omeka-S-module-DataTypeRdf/releases/download/3.4.14/CKEditorFootnotes-1.2.0-fix.tar.gz"
        }
    }
}
